<p id="notice"><%= notice %></p>

<h1>Dailycashes</h1>

<div class="media">
<div class="media-body">
<table class= "table table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>Sale</th>
      <th>Onlinepaid</th>
      <th>Discount</th>
      <th>Purchase</th>
      <th>Cashpaid</th>
      <th>Driverpaid</th>
      <th>Lodgement</th>
      <th>Overunder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dailycashes.each do |dailycash| %>
      <tr>
        <td><%= dailycash.date %></td>
        <td><%= dailycash.sale %></td>
        <td><%= dailycash.onlinepaid %></td>
        <td><%= dailycash.discount %></td>
        <td><%= dailycash.purchase %></td>
        <td><%= dailycash.cashpaid %></td>
        <td><%= dailycash.driverpaid %></td>
        <td><%= dailycash.lodgement %></td>
        <td><%= dailycash.overunder %></td>
        <td><%= link_to 'Show', dailycash, class: 'btn btn-mini'%></td>
        <td><%= link_to 'Edit', edit_dailycash_path(dailycash), class: 'btn btn-mini btn-success' %></td>
        <td><%= link_to 'Destroy', dailycash, method: :delete, class: 'btn btn-mini btn-danger', data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
<br>

<%= link_to 'New Dailycash', new_dailycash_path %>
